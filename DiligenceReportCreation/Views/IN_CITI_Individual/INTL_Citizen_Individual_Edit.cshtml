    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div class="panel-group" id="accordion">
            <div class="panel panel-default">
                <div class="panel-heading" id="PIID">
                    <h4 class="panel-title">
                        <a data-toggle="collapse" href="#collapse1">Personal Info</a>
                    </h4>
                </div>
                <div id="collapse1" class="panel-collapse collapse">
                    <div class="panel-body">
                        <label>
                            All fields with an asterisk (*) are required before you can "Submit Data" to create a report. The final report will reflect the information as it was entered here. Please be sure to double check spelling and use the correct capitalization/punctionation when entering data.
                        </label>
                        <table style="border-collapse: separate; border-spacing: 10px;">
                            <tr>
                                <th>Case Number</th>
                                <td>
                                    @ViewBag.CaseNumber
                                </td>
                                @*<td><input type="text" name="CaseNumber" required="required" /></td>*@
                            </tr>
                            <tr>
                                <th>First Name<span style="color:black"> *</span></th>
                                <td>
                                    @Html.EditorFor(model => model.diligenceInputModel.FirstName)
                                    @Html.ValidationMessageFor(model => model.diligenceInputModel.FirstName)
                                </td>
                                @*<td><input type="text" name="FirstName" required="required" /></td>*@
                            </tr>
                            <tr>
                                <th>Middle Name</th>
                                <td>
                                    @Html.EditorFor(model => model.diligenceInputModel.MiddleName)
                                    @Html.ValidationMessageFor(model => model.diligenceInputModel.MiddleName)
                                </td>
                            </tr>
                            <tr>
                                <th>Middle Initial</th>
                                <td>
                                    @Html.EditorFor(model => model.diligenceInputModel.MiddleInitial)
                                    @Html.ValidationMessageFor(model => model.diligenceInputModel.MiddleInitial)
                                </td>
                                @* <td><input type="text" name="MiddleInitial" required="required" /></td>*@
                            </tr>
                            <tr>
                                <th>Last Name</th>
                                <td>
                                    @ViewBag.LastName
                                </td>
                                @*<td><input type="text" name="LastName" required="required" /></td>*@
                            </tr>
                            <tr>
                                <th>Maiden Nme</th>
                                <td>
                                    @Html.EditorFor(model => model.diligenceInputModel.MaidenName)
                                    @Html.ValidationMessageFor(model => model.diligenceInputModel.MaidenName)
                                </td>
                            </tr>
                            <tr>
                                <th>Full Alias Name</th>
                                <td>
                                    @Html.EditorFor(model => model.diligenceInputModel.FullaliasName)
                                    @Html.ValidationMessageFor(model => model.diligenceInputModel.FullaliasName)
                                </td>
                            </tr>
                            <tr>
                                <th>Subject name in foreign language :</th>
                                <td>
                                    @Html.EditorFor(model => model.diligenceInputModel.foreignlanguage)
                                    @Html.ValidationMessageFor(model => model.diligenceInputModel.foreignlanguage)
                                </td>
                            </tr>
                            <tr>
                                <th>Date of Birth</th>
                                <td>
                                    @Html.TextBoxFor(model => model.diligenceInputModel.Dob, "{0:yyyy-MM-dd}", new { @class = "form-control", type = "date" })
                                    @Html.ValidationMessageFor(model => model.diligenceInputModel.Dob)
                                </td>
                            </tr>
                            <tr>
                                <th>Place of Birth</th>
                                <td>
                                    @Html.EditorFor(model => model.diligenceInputModel.Pob)
                                    @Html.ValidationMessageFor(model => model.diligenceInputModel.Pob)
                                </td>
                            </tr>
                            <tr>
                                <th>Country :</th>
                                <td>
                                    @Html.DropDownListFor(model => model.diligenceInputModel.Country, new SelectList(ViewBag.CountryList), new { @onchange = "callChangefunc(this.value)" })
                                    @Html.ValidationMessageFor(model => model.diligenceInputModel.Country)
                                </td>
                            </tr>
                            <tr>
                                <th>Nationality<span style="color:black"> *</span></th>
                                <td>
                                    @Html.EditorFor(model => model.diligenceInputModel.Nationality)
                                    @Html.ValidationMessageFor(model => model.diligenceInputModel.Nationality)
                                </td>
                            </tr>
                            <tr>
                                <th>Passport Number</th>
                                <td>
                                    @Html.EditorFor(model => model.diligenceInputModel.Nationalidnumber)
                                    @Html.ValidationMessageFor(model => model.diligenceInputModel.Nationalidnumber)
                                </td>
                            </tr>
                            <tr>
                                <th>Passport Issue Date</th>
                                <td>
                                    @Html.TextBoxFor(model => model.diligenceInputModel.Natlidissuedate, "{0:yyyy-MM-dd}", new { @class = "form-control", type = "date" })
                                </td>
                            </tr>
                            <tr>
                                <th>Passport Expiration Date</th>
                                <td>
                                    @Html.TextBoxFor(model => model.diligenceInputModel.Natlidexpirationdate, "{0:yyyy-MM-dd}", new { @class = "form-control", type = "date" })
                                </td>
                            </tr>
                            <tr>
                                <th>Additional Jurisdictions</th>
                                <td>
                                    @Html.EditorFor(model => model.diligenceInputModel.Nonscopecountry1)
                                    @Html.ValidationMessageFor(model => model.diligenceInputModel.Nonscopecountry1)
                                </td>
                            </tr>
                            <tr>
                                <th>Marital Status :</th>
                                <td>
                                    @Html.DropDownListFor(model => model.diligenceInputModel.Marital_Status, new List<SelectListItem>{
                        new SelectListItem {Text="Married",Value="Married"},new SelectListItem{Text="Never Married",Value="Never Married"},new SelectListItem {Text="Divorced",Value="Divorced"}})
                                </td>
                            </tr>
                            <tr>
                                <th>Children :</th>
                                <td>
                                    @Html.DropDownListFor(model => model.diligenceInputModel.Children, new List<SelectListItem>{
                        new SelectListItem {Text="No Children",Value="No Children"},new SelectListItem {Text="Children",Value="Children"}})
                                </td>
                            </tr>
                        </table>
                        <input type="submit" name="SaveData" value="SavePIData" onclick="callpichange()" />
                    </div>
                </div>
            </div>
